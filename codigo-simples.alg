// Seção de Declarações 
var
       n1, n2, s : inteiro
inicio
  Escreva("informe um numero: ")
  Leia(n1)
  Escreva("informe um numero: ")
  Leia(n2)
   s <- n1 + n2
   Escreva ("A soma  entre ", n1, "e" ,n2, "e igual ", s)
   
fimalgoritmo
